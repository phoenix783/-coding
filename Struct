#include<stdio.h>
struct employee 
{ char name [10];
  int age;
  int phone_no;
  int salary;
}
void main()
{ struct employee record[20];
  int i;
  for(i=0;i<=20;i++)
  { printf("name=%s",record.name);
    scanf("%c",&record.name);
    printf("age=%d",record.age);
    scanf("%d",&record.age);
    printf("phone_number=%d",record.phone_no);
    scanf("%d",&record.phone_no);
    printf("salary=%d",record.salary);
    scanf("%d",&record.salary);
  }
    strcpy(record.name,"John");
    record.age=24;
    record.phone_no=954574367;
    record.salary=45000;
    printf("name=%s age =%d phone_no=%d salary=%d\n",record.name,record.age,record.phone_no,record.salary);
    
    strcpy(record.name,"Paul");
    record.age=23;
    record.phone_no=9864367564;
    record.salary=67000;
    printf("name=%s age%d phone_no=%d salary=%d\n",record.name,record.age,record.phone_no,record.salary);

    strcpy(record.name,"Jones");
    record.age=89000;
    record.phone_no=9864324666;
    record.salary=89000;
    printf("name=%s age%d phone_no=%d salary=%d",record.name,record.age,record.phone_no,record.salary);
}
